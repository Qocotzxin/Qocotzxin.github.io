{
  "name": "github-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "start": "webpack-dev-server --mode development --open ",
    "build": "webpack --mode production",
    "lint": "./node_modules/.bin/eslint ./src"
  },
  "jest": {
    "transform": {
      "^.+\\.(jsx|js)?$": "<rootDir>/node_modules/babel-jest"
    },
    "testMatch": [
      "**/test/?(*.)+(spec|test).js?(x)"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "**/*.component.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "text-summary"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Qocotzxin/github-app.git"
  },
  "author": "Cristian Etchebarne",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Qocotzxin/github-app/issues"
  },
  "homepage": "https://github.com/Qocotzxin/github-app",
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.0",
    "eslint-config-jest-enzyme": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "node-sass": "^4.9.3",
    "react-test-renderer": "^16.5.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  }
}
